    <Page x:Class="Ginger.ActionLib.ActionOutputPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="600"
      Title="ActionOutputPage">

    <Grid>
        <DataGrid x:Name="ParamsDataGrid" Grid.Row="1" Grid.RowSpan="2" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Param" Binding="{Binding Path=Param}" />
                <DataGridTextColumn Header="Actual" Binding="{Binding Path=Actual}" />
                <DataGridTextColumn Header="Expected" Binding="{Binding Path=Expected}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
