<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
    <SccProjectName>SAK</SccProjectName>
    <SccProvider>SAK</SccProvider>
    <SccAuxPath>SAK</SccAuxPath>
    <SccLocalPath>SAK</SccLocalPath>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyVersion>0.0.0.0</AssemblyVersion>
    <FileVersion>0.0.0.0</FileVersion>
    <RootNamespace>Amdocs.Ginger.CoreNET</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <CodeAnalysisRuleSet>GingerCoreNET.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <CodeAnalysisRuleSet>GingerCoreNET.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="GeneralLib\JSON\**" />
    <EmbeddedResource Remove="GeneralLib\JSON\**" />
    <None Remove="GeneralLib\JSON\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="ALMLib\ExternalItemField.cs" />
    <Compile Remove="RecordingLib\ElementRecordEventArgs.cs" />
    <Compile Remove="RecordingLib\PageChangedEventArgs.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Reports\GingerExecutionReport\assets\css\bootstrap.css" />
    <None Remove="Reports\GingerExecutionReport\assets\css\font-awesome.css" />
    <None Remove="Reports\GingerExecutionReport\assets\css\styles.css" />
    <None Remove="Reports\GingerExecutionReport\assets\fonts\fontawesome-webfont.eot" />
    <None Remove="Reports\GingerExecutionReport\assets\fonts\fontawesome-webfont.ttf" />
    <None Remove="Reports\GingerExecutionReport\assets\fonts\SourceSansPro-Regular.ttf" />
    <None Remove="Reports\GingerExecutionReport\assets\img\@BeatLogo.png" />
    <None Remove="Reports\GingerExecutionReport\assets\img\@Ginger.png" />
    <None Remove="Reports\GingerExecutionReport\assets\img\@item_next.png" />
    <None Remove="Reports\GingerExecutionReport\assets\img\@item_prev.png" />
    <None Remove="Reports\GingerExecutionReport\assets\img\comments-icon.jpg" />
    <None Remove="Reports\GingerExecutionReport\assets\js\babel.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\bootstrap.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\circlechart.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\donutchart.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\jquery.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\prop-types.min.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\react-dom.min.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\react-with-addons.min.js" />
    <None Remove="Reports\GingerExecutionReport\assets\js\Recharts.min.js" />
    <None Remove="Reports\GingerExecutionReport\EmailExecutionReport.html" />
    <None Remove="Reports\GingerExecutionReport\Images\GingerLogo.png" />
    <None Remove="Reports\HTMLTemplates\SkeletonHTMLReport.html" />
    <None Remove="Reports\HTMLTemplates\SummaryHTMLReport.html" />
    <None Remove="RosLynLib\ValueExpressionRefrences.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Reports\GingerExecutionReport\assets\css\bootstrap.css">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\css\font-awesome.css">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\css\styles.css">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\fonts\fontawesome-webfont.eot">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\fonts\fontawesome-webfont.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\fonts\SourceSansPro-Regular.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\img\@BeatLogo.png" />
    <Content Include="Reports\GingerExecutionReport\assets\img\@Ginger.png" />
    <Content Include="Reports\GingerExecutionReport\assets\img\@item_next.png" />
    <Content Include="Reports\GingerExecutionReport\assets\img\@item_prev.png" />
    <Content Include="Reports\GingerExecutionReport\assets\img\comments-icon.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\babel.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\bootstrap.js">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\circlechart.js">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\donutchart.js">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\jquery.js">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\prop-types.min.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\react-dom.min.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\react-with-addons.min.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\assets\js\Recharts.min.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\EmailExecutionReport.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\GingerExecutionReport\Images\GingerLogo.png" />
    <Content Include="RosLynLib\ValueExpressionRefrences.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Reports\HTMLTemplates\SkeletonHTMLReport.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Reports\HTMLTemplates\SummaryHTMLReport.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
  
    <PackageReference Include="EPPlus" Version="4.5.3.1" />
	<PackageReference Include="LiteDB" Version="4.1.4" />
  
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.9.1" />
  
    <PackageReference Include="Microsoft.CodeAnalysis.Analyzers" Version="2.9.2">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="3.1.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.1.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="3.1.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Scripting" Version="3.1.0" />
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="System.Drawing.Common" Version="4.5.1" />
    <PackageReference Include="System.Reflection" Version="4.3.0" />
    <PackageReference Include="System.Reflection.DispatchProxy" Version="4.5.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\GingerCoreCommon\GingerCoreCommon.csproj" />
    <ProjectReference Include="..\GingerPluginCore\GingerPluginCore.csproj" />
    <ProjectReference Include="..\GingerUtils\GingerUtils.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="GingerExternal">
      <HintPath>DLLS\GingerExternal.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>

    <Compile Update="Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>

    <Compile Update="RosLynLib\ValueExpressionReference.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Update="Reports\GingerExecutionReport\assets\img\%40BeatLogo.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Update="Reports\GingerExecutionReport\assets\img\%40Ginger.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Update="Reports\GingerExecutionReport\assets\img\%40item_next.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Update="Reports\GingerExecutionReport\assets\img\%40item_prev.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>

    <None Update="Images\%40amdocs_logo.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>

    <None Update="Images\%40BeatLogo.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\%40GingerLogo_lowRes.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\%40ItemNext.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\%40ItemPrev.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="xcopy /e /y  &quot;$(ProjectDir)WebReportApp&quot; &quot;Reports\&quot;" />
  </Target>

</Project>
